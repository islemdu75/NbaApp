[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/islemdu75 (islemdu75)
[access "refs/for/refs/*"]
	read = group user/islemdu75 (islemdu75)
	push = group user/islemdu75 (islemdu75)
	label-Code-Review = -2..+2 group user/islemdu75 (islemdu75)
	label-Verified = -1..+1 group user/islemdu75 (islemdu75)
	rebase = group user/islemdu75 (islemdu75)
	removeReviewer = group user/islemdu75 (islemdu75)
	submit = group user/islemdu75 (islemdu75)
[access "refs/heads/*"]
	read = group user/islemdu75 (islemdu75)
	create = group user/islemdu75 (islemdu75)
	push = +force group user/islemdu75 (islemdu75)
	pushMerge = group user/islemdu75 (islemdu75)
[access "refs/tags/*"]
	push = group user/islemdu75 (islemdu75)
	push = +force group user/islemdu75 (islemdu75)
